    "unified",
    "remark",
    "slugger"
  ],
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/gitmahin/react-mdxutils.git"
  },
  "author": "Nimul Islam Mahin nimulmahin@gmail.com",
  "license": "MIT",
  "description": "Utilities for MDX content, including anchor extraction.",
  "dependencies": {
    "github-slugger": "^2.0.0",
    "mdast-util-toc": "^7.1.0",
    "react": "^19.1.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
