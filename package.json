{
  "name": "react-mdxutils",
  "version": "1.3.15",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup --format esm,cjs --dts --external react --minify",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
